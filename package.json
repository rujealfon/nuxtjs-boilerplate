{
  "name": "nuxtjs-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt.js Boilerplate",
  "author": "Ruje Alfon",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lintfix:js": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lintfix:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lintfix:all": "npm run lintfix:js && npm run lintfix:style",
    "precommit": "npm run lint-fix",
    "deploy": "deploy.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.8",
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/stylelint-module": "^3.2.2",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.7.0",
    "nuxt": "^2.15.8",
    "nuxt-vue-multiselect": "^1.0.4",
    "stylelint": "^13.3.3",
    "vue-toastr": "^2.1.2",
    "vuejs-datepicker": "^1.6.2",
    "vuelidate": "^0.7.4",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/moment": "^1.4.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "concurrent-transform": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "gulp-awspublish": "^4.1.1",
    "gulp-cloudfront-invalidate-aws-publish": "^1.0.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "stylelint-order": "^4.1.0"
  }
}
